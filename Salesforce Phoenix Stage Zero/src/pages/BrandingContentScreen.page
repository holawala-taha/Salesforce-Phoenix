                                <span class="slds-tabs--path__stage">
                                    <svg aria-hidden="true" class="slds-icon slds-icon--x-small">
                                        <use xlink:href="{!URLFOR($Resource.slds, '/assets/icons/utility-sprite/svg/symbols.svg#check')}"></use>
                                    </svg>
                                </span>
